{
  "name": "github-startz-angularjs-webpack",
  "version": "2.1.0",
  "description": "✨ GitHub Starz ✨ - An AngularJS ES6 components based app built with Webpack",
  "author": "Vitalii Burlaka <vitalii.burlaka@gmail.com>",
  "main": "dist/index.html",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --config webpack.config.dev.js --open",
    "lint": "eslint --ignore-path .gitignore --ext .js .",
    "lint:fix": "npm run lint -- --fix",
    "format": "npm run prettier -- --write",
    "prettier": "prettier \"**/*.{json,md,css,scss,html}\"",
    "build": "cross-env NODE_ENV=production webpack -p --progress --config webpack.config.prod.js",
    "analyze": "npm run build -- --env.analyze"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=5"
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.22",
    "angular": "^1.7.8",
    "angular-animate": "^1.7.8",
    "core-js": "^3.1.4",
    "normalize.css": "^8.0.1",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "angular-mocks": "^1.7.8",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-angularjs-annotate": "^0.10.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "file-loader": "^4.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "ngtemplate-loader": "^2.0.1",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.1",
    "prettier": "^1.18.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.36.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --ignore-path .gitignore --fix",
      "git add"
    ],
    "*.{json,md,css,scss,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ]
}
