{
  "name": "angularjs-webpack-seed",
  "version": "1.0.0",
  "description": "AngularJS components app built with Webpack",
  "author": "@vitaliiburlaka",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --progress --open --config webpack.config.dev.js",
    "lint": "eslint ./src --cache",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean&& npm run lint",
    "build": "cross-env NODE_ENV=production webpack -p --progress",
    "build:dev": "npm run clean&& webpack -p --progress",
    "analyze": "npm run clean&& cross-env NODE_ENV=production webpack -p --progress --env.analyze"
  },
  "license": "MIT",
  "dependencies": {
    "@uirouter/angularjs": "^1.0.20",
    "angular": "^1.7.5",
    "angular-animate": "^1.7.5",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "angular-mocks": "^1.7.5",
    "autoprefixer": "^9.3.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.9",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "ng-annotate-loader": "^0.6.1",
    "ngtemplate-loader": "^2.0.1",
    "node-sass": "^4.10.0",
    "postcss-loader": "^2.1.0",
    "prettier": "1.15.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.20.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "^2.11.1"
  }
}
