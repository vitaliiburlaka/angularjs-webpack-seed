{
  "name": "github-startz-angularjs-webpack",
  "version": "2.0.0",
  "description": "✨ GitHub Starz ✨ - An AngularJS ES6 components based app built with Webpack",
  "author": "Vitalii Burlaka <vitalii.burlaka@gmail.com>",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --config webpack.config.dev.js --open",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean && npm run lint",
    "build": "cross-env NODE_ENV=production webpack -p --progress",
    "analyze": "npm run build -- --env.analyze"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=5"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@uirouter/angularjs": "^1.0.20",
    "angular": "^1.7.5",
    "angular-animate": "^1.7.5",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "angular-mocks": "^1.7.5",
    "autoprefixer": "^9.4.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-angularjs-annotate": "^0.10.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "ngtemplate-loader": "^2.0.1",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-safe-parser": "^4.0.1",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.1",
    "url-loader": "^1.1.2",
