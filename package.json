{
  "name": "angularjs-webpack-seed",
  "version": "1.0.0",
  "description": "AngularJS components app built with Webpack",
  "author": "@vitaliiburlaka",
  "main": "src/index.js",
  "scripts": {
    "lint": "echo \"Linting code...\"&& eslint ./src --cache --ignore-pattern .gitignore",
    "postlint": "echo \"Everything looks fine :)\"",
    "prestart": "npm run lint",
    "start": "webpack-dev-server --progress --open --config webpack.config.dev.js",
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean&& npm run lint",
    "build": "cross-env NODE_ENV=production webpack -p --progress",
    "build:dev": "npm run clean&& webpack -p --progress",
    "analyze": "npm run clean&& cross-env NODE_ENV=production webpack -p --progress --env.analyze"
  },
  "license": "MIT",
  "dependencies": {
    "angular": "^1.6.9",
    "angular-animate": "^1.6.9",
    "angular-ui-router": "^0.4.3",
    "normalize.css": "^7.0.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.9",
    "autoprefixer": "^8.6.3",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "ng-annotate-loader": "^0.6.1",
    "ngtemplate-loader": "^2.0.1",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.0",
    "prettier": "1.10.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "^2.11.1"
  }
}
